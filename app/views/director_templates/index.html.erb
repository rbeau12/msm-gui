<form action="/directors" method="post">
  <div><label for="name">Name</label><input id="name" type="text" name="name"></div>
  <div><label for="dob">Dob</label><input id="dob" type="date" name="dob"></div>
  <div><label for="bio">Bio</label><input id="bio" type="textarea" rows="3" name="bio"></div>
  <div><label for="image">Image<input id="image" name="image" type="text"></div>
  <button>Create director</button>
</form>
  



<h1>
  List of all directors
</h1>

<hr>

<table border="1">
  <tr>
    <th>
      ID
    </th>

    <th>
      Image
    </th>

    <th>
      Name
    </th>

    <th>
      Dob
    </th>

    <th>
    </th>
  </tr>

  <% @list_of_directors.each do |director| %>
  <tr>
    <td>
      <%= director.id %>
    </td>

    <td>
      <img src="<%= director.image %>" style="width:250px;">
    </td>

    <td>
      <%= director.name %>
    </td>

    <td>
      <%= director.dob %>
    </td>

    <td>
      <a href="/directors/<%= director.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
